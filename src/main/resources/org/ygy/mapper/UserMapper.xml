<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="org.ygy.mapper.UserMapper">

	<resultMap type="org.ygy.entity.RegisterVO" id="registerMapper">
		<result column="calendar_id" property="calendarId"/>
		<result column="cust_register_num" property="todayAddNum"/>
		<result column="cust_register_num_total" property="totalNum"/>
	</resultMap>
  
    <select id="selectUser" resultType="org.ygy.entity.UserEntity">  
        select count(1) id ,'cal' name from dr_bi.dm_calendar
    </select>  
    
    <select id="selectRegister" resultMap="registerMapper">
	    select 
			calendar_id ,
			sum(cust_register_num) cust_register_num,
			sum(cust_register_num_total) cust_register_num_total
		from 
			dm_cust_channel_analyse
		group by 
			calendar_id
		order by 
			calendar_id desc
    </select>
    
    <!-- 

     -->
</mapper> 